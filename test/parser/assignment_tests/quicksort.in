fun quicksort(l : List) : List
{
	Integer m = l.first();
	l.pop();
	List a = newList();
	List b = newList();
	for(Integer i = 1..(l.range())){
		if(l.get(i) > m){
			a.put(l.get(i));
		}else{
			(b.put(l.get(i)));
		}
	}
	List newList = flatten(a,m,b);

	# will not terminate trololololo
	quicksort(newList);
}
return 0;