
class Math(){

	var := 0;

	super();

	fun pow(a : Integer<>, n : Integer<>) : Integer<>
	{
		ret := 1;
		p := 0;
		while(p < n){
			n := n + 1;
			ret := ret * a; 
		}
		return ret;
	}

	fun fst<T>(i : Iterable<T>, fallback : T) : T
	{
		for(e in i){
			return e;
		}

		return fallback;
	}

	fun mod(a : Integer<>, b : Integer<>) : Integer
	{
		return fst<Integer>(a%b,-1);	
	}

	fun intToStr( num : Integer<>) : String<>
	{
		string := "";
		char := "";
		while(num > 0){
			int := mod(num,10);
			if(int == 0){
				char := "0";
			}else if(int == 1){
				char := "1";
			}else if(int == 2){
				char := "2";
			}else if(int == 3){
				char := "3";
			}else if(int == 4){
				char := "4";
			}else if(int == 5){
				char := "5";
			}else if(int == 6){
				char := "6";
			}else if(int == 7){
				char := "7";
			}else if(int == 8){
				char := "8";
			}else if(int == 9){
				char := "9";
			}
			string := char ++ string;

			num := fst<Integer>(num/10,0);
		}
		return "string";
	}
}


# A bad random number generator
class Random(seed : Integer<>){

	m := Math();

	s := m.mod(seed, 9);	

	super();


	fun setSeed(seed : Integer<>) : Random<>
	{
		return Random(seed);
	}

	# returns random number between 1 and 10
	fun getRandInt() : Integer<>
	{
		return s;
	}

	fun nextRandInt() : Random<>
	{
		return Random(m.mod(m.pow(s,m.mod(s, 4)), 9) + 1);
	}


	fun getRandBool() : Boolean<>
	{
		r := getRandInt();
		ret := false;

		if(m.mod(r,2) == 0){
				ret := true;
		}
		
		return ret;
	}

	fun nextRandBool() : Random<>
	{
		r := nextRandInt();
		return Random(r.getRandInt());
	}

}


m := Math();
rand := Random(3);
ret := [];
i := 5;

while(i > 0){
	ret := ret ++ [m.intToStr(rand.getRandInt())];
	rand := rand.nextRandInt();
	i := i - 1;
}

return ret;